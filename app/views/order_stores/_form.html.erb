<%= form_with(model: order_store, local: true) do |form| %>
  <% if order_store.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(order_store.errors.count, "error") %> prohibited this order_store from being saved:</h2>

      <ul>
        <% order_store.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :value %>
    <%= form.text_field :value %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.text_field :status %>
  </div>

  <div class="field">
    <%= form.label :order_date %>
    <%= form.datetime_select :order_date %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
